// Infinite loop
for { }

// Infinite loop the increments i
for i { }

// Loop in range
for i in 5..10 {  }

// Loop in range with steps
for i in 5..10 step 2 { }

// Loop until number
for i in ..10 { }

// Normal while loop
for x > y { }

// Normal while loop with index
for i, x > y { }

// foreach loop list
for obj in list { }

// foreach loop list  with index
for i, obj in list { }

// foreach map loop
for k, v in map { }
